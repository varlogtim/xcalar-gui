@color-scratchpad-selected: rgba(175, 28, 237, 0.1);
@color-scratchpad-purple: rgba(99, 190, 168, 0.1);

/* Fonts */
@font-base: "Open Sans", "Trebuchet MS", Arial, sans-serif;
@font-code: "Source Code Pro", courier, sans-serif;

