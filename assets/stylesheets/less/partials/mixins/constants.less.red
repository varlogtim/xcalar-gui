 /* ========== Constants ========== */

 /* Standard */
@color-white: #FFFFFF;
@color-black: #000000;

/* Shadow */
@color-shadow-darkGray: #AFA7A6;
@color-shadow-black: rgba(0, 0, 0, 0.15);

/* Bar */
@color-bar-orange: #3CFDBC;
@color-bar-darkGray-bot: #8C8C8C;
@color-bar-darkGray-top: #A3A3A3;
@color-bar-border: #312424;
@color-error-border: #BDFDE7;

/* Monitor */
@color-monitor-red: #A7F8E1;
@color-monitor-green: #DC95CC;
@color-monitor-lightGreen: #F0D4EB;
@color-monitor-midGreen: #EDA6DD;
@color-monitor-blue: #D2A6A7;

/* Tabs */
@color-tab-border: #6A4C4A;
@color-tab-borderBlue: #7B2638;
@color-tab-divider: #838383;

/* Backgrounds */
@color-background-white: #FFFFFF;
@color-background-topAction: #E8635B;
@color-background-bottomAction: #B33639;
@color-background-lightGray: #9C9C9C;
@color-background-darkGray: #E6E6E6;
@color-background-lightLightGray: #DDDDDD;
@color-background-topGrayMed: #CCCCCC;
@color-background-midGrayMed: #BEBEBE;
@color-background-bottomGrayMed: #AEAEAE;
@color-background-darkdarkGray: #666666;
@color-background-columnList: #CDCDCD;
@color-background-topForm: #F0F0F0;
@color-background-sidebar: #999999;
@color-background-tableBorders: #E6E6E6;
@color-background-bookmark: #FF0B00;
@color-background-input: #909090;
@color-background-input-active: #E6E6E6;
@color-background-icon-darkGray: #C6C6C6;
@color-background-colIcon-lightGray: #BABABA;
@color-background-colIcon-gray: #A0A0A0;
@color-background-colIcon-lightBlue: #D4544B;
@color-background-colIcon-blue: #A6302F;
@color-triangle-colIcon-blue: #A13A35;

/* Login */
@color-login-field-top: #D9D9D9;
@color-login-field-bot: #A0A0A0;

/* Join Modal */
@color-join-background: #F0F0F0;
@color-join-divider: #D7D7D7;
@color-join-border: #A8A8A8;
@color-join-drop-shadow: #B4B4B4;

/* Retina Modal */
@color-retina-border: #CECECE;

/* Operation Modal */
@color-circle-border: #BFBFBF;
@color-circle-background: #FAFAFA;
@color-innerCircle-background: #C54544;
@color-innerCircle-text: #EEEEEE;

/* Inputs */
@color-input-border-lightGray: #EAEAEA;
@color-input-border-midGray: #EEEEEE;

/* Dropdown menu colors */
@color-selectedCell-dark: #E6D2D1;
@color-selectedCell-light: #F2DEDD;
@color-selectedCell-green: #E4D3D6;
@color-dropBox: #932122;
@color-dropBox-secondary: #C7A3A1;
@color-dropBox-tertiary: #A76774;

/* Text */
@color-text-black: #0F0F0F; 
@color-text-white: #FFFFFF; 
@color-text-transparentWhite: rgba(255, 255, 255,0.95); 
@color-text-brightBlue: #E8635B;
@color-text-blue: #B6383A;
@color-text-tab: #8C7070;
@color-text-darkGray: #4D4D4D;
